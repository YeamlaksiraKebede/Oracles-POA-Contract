{
    "extends": "default",
    "rules": {
        "not-rely-on-time": false,
        "no-complex-fallback": "warn",
        "indent": [
            "error",
            4
        ],
        "quotes": [
            "error",
            "double"
        ],
        "max-line-length": [
            "error",
            120
        ]
    }
}
